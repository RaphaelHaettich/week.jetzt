bs_package_flags = -bs-package-name react-template -bs-package-map ReactTemplate
src_root_dir = /mnt/c/git/weekjetzt
bsc = /mnt/c/git/weekjetzt/node_modules/bs-platform/lib/bsc.exe
bsdep = /mnt/c/git/weekjetzt/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/mnt/c/git/weekjetzt/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = -I /mnt/c/git/weekjetzt/node_modules/reason-react/lib/ocaml
bs_package_dev_includes = 
namespace = -ns ReactTemplate
bsb_dir_group = 0
refmt = /mnt/c/git/weekjetzt/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -ppx '/mnt/c/git/weekjetzt/node_modules/bs-platform/lib/reactjs_jsx_ppx_2.exe'
refmt_flags = --print binary
bsc_lib_includes = -I . -I src/components -I src/helper -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Index.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/Index.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/Index.mlast.d : build_deps src/Index.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/Index-ReactTemplate.cmj |  $src_root_dir/src/Index.bs.js src/Index-ReactTemplate.cmi : build_cmj_cmi src/Index.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/helper/WeekOfYear.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/helper/WeekOfYear.re
build src/helper/WeekOfYear.mlast.d : build_deps src/helper/WeekOfYear.mlast
build src/helper/WeekOfYear-ReactTemplate.cmj |  $src_root_dir/src/helper/WeekOfYear.bs.js src/helper/WeekOfYear-ReactTemplate.cmi : build_cmj_cmi src/helper/WeekOfYear.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src/helper
build src/components/Component1.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/components/Component1.re
build src/components/Component1.mlast.d : build_deps src/components/Component1.mlast
build src/components/Component1-ReactTemplate.cmj |  $src_root_dir/src/components/Component1.bs.js src/components/Component1-ReactTemplate.cmi : build_cmj_cmi src/components/Component1.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src/components
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -bs-cmi-only -c ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build ReactTemplate.cmi : build_package ReactTemplate.mlmap
build build.ninja : phony  ||  ReactTemplate.cmi src/components/Component1.mlast.d src/helper/WeekOfYear.mlast.d src/Index.mlast.d
